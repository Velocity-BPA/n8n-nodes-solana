{
  "name": "n8n-nodes-solana",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Solana blockchain providing 8 resources and 32+ operations for accounts, transactions, SPL tokens, NFTs, staking, and more. Includes WebSocket triggers for real-time events.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "solana",
    "blockchain",
    "web3",
    "spl-token",
    "nft",
    "metaplex",
    "defi",
    "cryptocurrency"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-solana",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-solana.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SolanaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Solana/Solana.node.js",
      "dist/nodes/Solana/SolanaTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "bs58": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
